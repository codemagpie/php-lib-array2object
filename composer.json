{
    "name": "codemagpie/array2object",
    "type": "library",
    "description": "A high performance, Supports converting array into object and object into array, easy to use",
    "keywords": [
        "codemagpie",
        "array",
        "object",
        "array2object",
        "array to object",
        "convert"
    ],
    "authors": [
        {
            "name": "lyf",
            "email": "1191628562@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/property-info": "^5.4",
        "hyperf/utils": "^2.2",
        "codemagpie/utils": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5",
        "ext-json": "*"
    },
    "license": "MIT",
    "archive": {
        "exclude": ["/tests", ".php-cs-fixer.php"]
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "prefer-stable": true,
    "config": {
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "CodeMagpie\\ArrayToObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMagpie\\ArrayToObjectTests\\": "tests/"
        }
    }
}
