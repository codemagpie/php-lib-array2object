{
    "name": "codemagpie/array2object",
    "type": "library",
    "keywords": ["codemagpie", "array", "object", "array2object", "array to object", "convert"],
    "authors": [
        {
            "name": "lyf",
            "email": "1191628562@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/property-info": "^5.4",
        "hyperf/utils": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5",
        "ext-json": "*"
    },
    "license": "MIT",
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "prefer-stable": true,
    "config": {
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "CodeMagpie\\ArrayToObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMagpie\\ArrayToObjectTests\\": "tests/"
        }
    }
}
